<h1>Edit Song Form</h1>

<%= form_for @song do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br></br>

  <%= f.label "Artists" %>
  <%= f.text_field :artist_id %>
  <!-- <%= f.collection_radio_buttons :artist_id, @artists, :id, :name %> -->
  <br></br>

  <%= f.label "Genres" %>
  <%= f.text_field :genre_id %>
  <!-- <%= f.collection_radio_buttons :genre_id, @genres, :id, :name %> -->
  <br></br>

  <%= f.submit %>
<% end %>
