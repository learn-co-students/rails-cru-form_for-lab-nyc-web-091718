<h1>Edit information for <%= @song.name %></h1>

<%= form_for(@song) do |f| %>
  <label>Song Name:</label>
  <%= f.text_field :name %><br><br>

  <label>Artist ID:</label>
  <%= f.number_field :artist_id %><br><br>

  <label>Genre ID:</label>
  <%= f.number_field :genre_id %><br><br><br>

  <%= f.submit %>
<% end %>
