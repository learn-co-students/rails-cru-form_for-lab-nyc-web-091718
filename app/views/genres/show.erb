<h1><%= @genre.name %></h1>

<h2><%= @genre.name %>'s Songs</h2>
<% @songs.uniq.each do |song| %>
  <%= link_to song.name, song_path(song) %><br>
<% end %>

<h2><%= @genre.name %>'s Artists</h2>
<% @artists.uniq.each do |artist| %>
  <%= link_to artist.name, artist_path(artist) %><br>
<% end %>

<h2>Navigation Links</h2>

<%= link_to "Genre Index", genres_path %><br>
<%= link_to "Edit Genre", edit_genre_path %><br>
